{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./",
    "skipLibCheck": true,
    "allowImportingTsExtensions": false,
    "noEmit": false,
    "noImplicitAny": false,
     "esModuleInterop": true
  },
  "ts-node": {
    "esm": true
  },
"include": ["."],
"exclude": ["node_modules", "dist"]
  
}
